
#include <iostream>
#include <string>
using namespace std;

string username, password;

// fungsi sign up
void signUp() {
    cout << endl << "    SIGN UP    " << endl;
    cout << "Masukkan username : ";
    cin >> username;
    cout << "Masukkan password : ";
    cin >> password;
    cout << "Sign up berhasil! Silakan sign in." << endl;
}
